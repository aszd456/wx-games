.container {
	height: 100vh;
	color: #E64340;
	font-weight: bold;
	display: flex;
	flex-direction: column;
	align-items: center;
	/**均匀排列每个元素,每个元素之间的间隔相等****/
	justify-content: space-evenly;
}

.title {
	font-size: 18pt;
}

.levelBox {
	width: 100%;
}

.box {
	width: 50%;
	float: left;
	margin: 25rpx 0;
	display: flex;
	flex-direction: column;
	align-items: center;
}

image {
	width: 260rpx;
	height: 260rpx;
}
